{
  "name": "secure-password-manager",
  "version": "1.0.0",
  "description": "Zero-Knowledge Password Manager with Advanced Security",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [
    "password-manager",
    "security",
    "encryption",
    "zero-knowledge"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "nodemailer": "^7.0.10",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
